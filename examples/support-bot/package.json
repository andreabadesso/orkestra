{
  "name": "support-bot-example",
  "version": "0.0.1",
  "private": true,
  "description": "Support bot example demonstrating AI-first with human escalation",
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsx watch src/index.ts",
    "worker": "tsx src/worker.ts",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo *.tsbuildinfo"
  },
  "dependencies": {
    "@orkestra/sdk": "workspace:*",
    "@orkestra/core": "workspace:*",
    "@temporalio/client": "^1.14.1",
    "@temporalio/worker": "^1.14.1",
    "@temporalio/workflow": "^1.14.1",
    "@temporalio/activity": "^1.14.1",
    "express": "^4.21.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.7.2",
    "tsx": "^4.19.0"
  }
}
