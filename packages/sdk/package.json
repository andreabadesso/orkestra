{
  "name": "@orkestra/sdk",
  "version": "0.0.1",
  "description": "Developer-friendly workflow helpers for Orkestra",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "test": "vitest run",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo *.tsbuildinfo"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@temporalio/workflow": "^1.14.1"
  }
}
